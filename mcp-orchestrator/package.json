{
  "name": "claude-mgr-orchestrator",
  "version": "1.0.0",
  "description": "MCP server for orchestrating Claude Manager agents",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "claude-mgr-orchestrator": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && esbuild dist/index.js --bundle --platform=node --format=esm --outfile=dist/bundle.js --banner:js=\"import{createRequire}from'module';const require=createRequire(import.meta.url);\"",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
